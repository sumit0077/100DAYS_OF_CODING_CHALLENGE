import java.util.*;

class LinearSearch {
	public static void main(String[] args) {
		int[] arr = { 10, 2, 5, 8, 9, 3, 4, 7 };
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter any key for search");
		int key = sc.nextInt();
		int i;

		for (i = 0; i < arr.length; i++) {

			if (arr[i] == key) {
				System.out.println("Element is " + (i + 1) + " index position");
				break;

			}

		}
		if (i == arr.length) {
			System.out.println("Element index not found");
		}
	}

}
